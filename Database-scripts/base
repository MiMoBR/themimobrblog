CREATE TABLE BLOG_CONTENT (
	ID int IDENTITY(1,1) PRIMARY KEY,
	TITLE VARCHAR(150),
	SUBTITLE VARCHAR(250),
	CONTENT VARCHAR(MAX),
	PIC1 VARCHAR(300),	
	PIC2 VARCHAR(300),
	URL1 VARCHAR(300),
	URL2 VARCHAR(300),
	SEOLOGO VARCHAR(300),
	SEOSITE VARCHAR(300),
	SEOTYPE VARCHAR(50),
	SEOAUTHOR VARCHAR(100),
	SEOCOMPANY VARCHAR(100),
	SEOTELEPHONE VARCHAR(100),
	SEOADDRESS VARCHAR(100),
	SEOLOCALITY VARCHAR(100),
	SEOCOUNTY VARCHAR(100),
	SEOCOUNTRY VARCHAR(100),
	SEOPOSTECODE VARCHAR(100),
	SEOHASMAP VARCHAR(300),
	SEOTWITTER VARCHAR(100),
	SEOOPENINGHOURS VARCHAR(150),
	URL VARCHAR(300) NOT NULL UNIQUE,
	DT_CREATED DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP, 
);

ALTER TABLE BLOG_CONTENT ALTER COLUMN SEOHASMAP VARCHAR(300)
ALTER TABLE BLOG_CONTENT ALTER COLUMN SEOTWITTER VARCHAR(100)
ALTER TABLE BLOG_CONTENT ALTER COLUMN SEOOPENINGHOURS VARCHAR(150)